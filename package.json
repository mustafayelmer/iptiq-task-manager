    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^9.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.4"
  },
